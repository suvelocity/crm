  "scripts": {
    "fuckin:go": "npm i && npm run schema && npm run migrate && npm run seed && npm run dev ",
    "start": "node out/.",
    "test": "NODE_ENV=test jest --verbose --forceExit --detectOpenHandles",
    "spintest": "NODE_ENV=test npm run migrate && npm run test",
    "build": "tsc --build tsconfig.json",
    "migrate": "cd src && sequelize db:migrate",
    "spinup": "npm run migrate && npm run seed && npm run dev",
    "migrate:prod": "cd out && sequelize db:migrate",
    "spinup:staging": "npm run migrate:prod && npm run seed:prod && npm run start",
    "spinup:prod": "npm run migrate:prod && npm run start",
    "dev": "nodemon -r dotenv/config ./src/index.ts",
    "seed": "cd src && sequelize db:seed:undo:all && npx sequelize db:seed:all",
    "schema": "cd src && sequelize db:create ",
    "seed:prod": "cd out && sequelize db:seed:undo:all && npx sequelize db:seed:all",
    "fetch": "cd src && sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all"
  },
